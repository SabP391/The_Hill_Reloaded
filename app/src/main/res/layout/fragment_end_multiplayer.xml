<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout16"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/giallo_scuro_background"
    tools:context=".EndMultiplayerFragment">

    <ImageView
        android:id="@+id/bg_multiplayer_image"
        android:layout_width="731dp"
        android:layout_height="372dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bg_main" />

    <ImageView
        android:id="@+id/sole_rotante_sp2"
        android:layout_width="67dp"
        android:layout_height="62dp"
        android:layout_marginTop="156dp"
        android:src="@drawable/ic_sole"
        app:layout_constraintBottom_toBottomOf="@+id/bg_multiplayer_image"
        app:layout_constraintEnd_toEndOf="@+id/bg_multiplayer_image"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.672" />

    <TextView
        android:id="@+id/processing_scores"
        android:layout_width="456dp"
        android:layout_height="27dp"
        android:text="@string/processo_punteggi"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/sole_rotante_sp2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.687" />
</androidx.constraintlayout.widget.ConstraintLayout>